<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:t="sap.ui.table">
	<t:Table rows="{/products}">
		<t:columns>
			<t:Column>
				<t:label>
					<Text text="Product Name"></Text>
				</t:label>
				<t:template>
					<Text text="{Name}"></Text>
				</t:template>
			</t:Column>
		</t:columns>
	</t:Table>
</core:FragmentDefinition>